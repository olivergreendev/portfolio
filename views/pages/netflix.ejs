                                </div>
                                <div class="other-projects--project-item__image">
                                    <img src="/img/app-placeholder.png" alt="">
                                </div>
                            </div>
                        </a>
                    </div>
                </div>
            </div>

        </section>
        <!-- Background Glows -->
        <div class="bg-glow top right"></div>
        <div class="bg-glow bottom left"></div>
    </main>
    <%- include('../partials/_footer.ejs') %>

    <script src="/js/client-sidenav.js" async defer></script>
    <script src="/js/client-bg-glow-fade.js" async defer></script>
